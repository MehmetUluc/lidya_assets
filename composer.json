{
    "name": "lidyapos/assets",
    "description": "Laravel assets management",
    "license": "MIT",
    "keywords": [
        "lidyapos",
        "lidyapos cms",
        "lidyapos platform",
        "lidyapos assets"
    ],
    "homepage": "https://lidyapos.com",
    "authors": [
        {
            "name": "Lidyapos",
            "email": "info@lidyapos.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "laravel/framework": "^5.6|^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0|^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LidyaPos\\Assets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LidyaPos\\Assets\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LidyaPos\\Assets\\Providers\\AssetsServiceProvider"
            ],
            "aliases": {
                "Assets": "LidyaPos\\Assets\\Facades\\AssetsFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
